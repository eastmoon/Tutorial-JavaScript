<!-- views/pages/home.ejs -->
<!DOCTYPE html>
<html>
<head>
  <% include src/views/partials/head %>
</head>
<body class="container">
  <header>
    <% include src/views/partials/header %>
  </header>

  <main>
    <div class="jumbotron">
      <h1>Check this out</h1>
      <p>It's using EJS template</p>
      <ul>
        <li><%= htmlWebpackPlugin.options.title %></li>
        <li>
          <%= htmlWebpackPlugin.options.chunks[0] %>
          <ul>
            <li><%= htmlWebpackPlugin.files.chunks[htmlWebpackPlugin.options.chunks[0]].entry %></li>
            <li><%= htmlWebpackPlugin.files.chunks[htmlWebpackPlugin.options.chunks[0]].css %></li>
          </ul>
        </li>
        <li><%= htmlWebpackPlugin.options.filename %></li>
      </ul>
    </div>
  </main>

  <footer>
    <% include src/views/partials/footer %>
  </footer>
</body>
</html>
